@model IEnumerable<PhanCongGiangDay.Models.GiaiDoan>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GiaiDoans</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenGiaiDoan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgayBatDau)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgayKetThuc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaLopHp)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenGiaiDoan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NgayBatDau)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NgayKetThuc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaLopHp)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaGiaiDoan">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaGiaiDoan">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaGiaiDoan">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
