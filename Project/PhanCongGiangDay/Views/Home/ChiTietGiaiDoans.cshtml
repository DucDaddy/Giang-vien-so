@model IEnumerable<PhanCongGiangDay.Models.ChiTietGiaiDoan>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ChiTietGiaiDoans</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Thu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tiet)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaGiaiDoan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaPhong)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Thu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tiet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaGiaiDoan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaPhong)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaCtgd">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaCtgd">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaCtgd">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
